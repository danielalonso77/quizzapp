    <h1>Vista para realizar los examenes</h1>
    <p>Contendrá un molde de los examenes creados para que los contesten los alumnos</p>

  

    <h1><%= quizz.nombreQuizz %></h1>
    <br>
        <form action="#">

       <% 
            for(var i=0; i<quizz.cuestionario.length ; i++)
                {%>
                    <% switch (quizz.cuestionario[i].tipo) {
                        case "tipoT" : %>
                        <h4 align="center"><%= quizz.cuestionario[i].pregunta %></h4>
                        <input type="text" class="form-control" placeholder="Respuesta" aria-label="Respuesta" name="respuesta1">
                        <br>
                                <% break;
                        case "tipoR" : %>
              
                        <h4>Relaciona los items</h4>

                        <div id="main">
                            <table class="tablaEjercicio">

                              <% for (var j = 0; j<quizz.cuestionario[i].pregunta.length; j++) {%>
                                <tr>
                                  <!-- <td class="contestada">1.	Motivo por el cual Marx critica la propuesta dialéctica de Hegel: </td> -->
                                  <td>1.<%= quizz.cuestionario[i].pregunta[j]%></td>
                                  <td class="boton"><button></button></td>
                                  <td class="celdaRespuesta"><%= quizz.cuestionario[i].respuesta[j]%></td>
                                </tr>
                              <% } %>

                            </table>
                            <input id="botonReiniciar" type="button" value="Otra vez" class="btn_reiniciar" />
                            <div class="dialog-background" id="retroFinal" style="display: none;">
                                <div class="dialog-loading-wrapper">
                                    <div class="dialog-loading-icon">
                                        <img id="botonCerrarRetro" src="data:image/png;base64," alt="cerrar aviso" />
                                        <div id="retroFinalMensaje">
                                            <!-- Aquí se colocarán los mensajes de retroalimentación. -->
                                            ¿probando?
                                        </div>
                                    </div>
                                </div>
                            
                            </div>
                        
                        </div>
                                <% break;
                        case "tipoOM" : %>

                        <%
                        <!-- solo una respuesta posible-->
                        if (quizz.cuestionario[i].respuesta.length<=1) { %>
                            <h4 align="center"><%= quizz.cuestionario[i].pregunta[0]%></h4>
                            <% for (var j = 1; j<quizz.cuestionario[i].pregunta.length; j++) {%>

                              <div class="form-check">
                                <input class="form-check-input" type="radio" name="pregunta" id="flexRadioDefault1">
                                <label class="form-check-label" for="pregunta">
                                  <%= quizz.cuestionario[i].pregunta[j]%>
                                </label>
                              </div>
                            <% } %>

                          
                        <% }
                        <!-- Multiples Respuestas-->
                        else { %>
                              <h4 align="center"><%= quizz.cuestionario[i].pregunta[0] %></h4>

                              <% for (var j = 1; j<quizz.cuestionario[i].pregunta.length; j++) {%>
                                <input type="checkbox"  name="pregunta" value="<%= quizz.cuestionario[i].pregunta[j] %>">
                                <label for="pregunta"><%= quizz.cuestionario[i].pregunta[j] %></label><br>
                                <br>
                              <% } %>

                            <% }
                        %>  
                               
                     
                                <% break;
                        case "tipoIT" : %>
                        <h4 align="center"><%= quizz.cuestionario[i].pregunta[1] %></h4>
                        <div align="center">
                          <img src="/image/<%= quizz.cuestionario[i].pregunta[0] %>" alt="perrito de la victoria" >
                        </div>
                        
                        <input type="text" class="form-control" placeholder="Respuesta" aria-label="Respuesta" name="respuesta1">
                                <% break;

                        case "tipoM" : %>
                        <h4 align="center">Resuelve la siguiente expresión</h4>
                        <div class="container">  
                          <div class="row justify-content-center" style="background-color:lightblue">
                            <p class="mate"><%= quizz.cuestionario[i].pregunta[0] %> <p></p>
                           
                    
                          </div>  
                      </div>
                        
                       
                                <% break;
                            
                        
                        
                        } 
                      %>
                <% } 
       %>
